{
  "name": "bloodlog/webinar-client",
  "description": "PHP client for webinar.ru",
  "keywords": ["client", "api", "webinar.ru", "webinar"],
  "support": {
    "issues": "https://github.com/Bloodlog/webinar-client/issues",
    "source": "https://github.com/Bloodlog/webinar-client/issues"
  },
  "homepage": "https://github.com/Bloodlog/webinar-client",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^6.2|^7.0.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "orchestra/testbench": "6.*"
  },
  "autoload": {
    "psr-4": {
      "Bloodlog\\WebinarClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "extra" : {
    "laravel" : {
      "providers" : [
        "Bloodlog\\WebinarClient\\Providers\\WebinarClientProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Bloodlog",
      "email": "fm@web-fomin.ru",
      "homepage": "https://web-fomin.ru",
      "role": "Developer"
    }
  ]
}
